import requests

url = "https://dicionario.priberam.org/casa"
response = requests.get(url)

if response.status_code == 200:
    xml_data = response.text

    with open("agulha.txt", "w") as file:
        file.write(xml_data)
        print("XML data saved to output.txt")
else:
    print("Failed to retrieve XML data from the URL")